# Microsoft Entra (Azure AD) Configuration
REACT_APP_AZURE_CLIENT_ID=your_azure_client_id_here
REACT_APP_AZURE_TENANT_ID=your_azure_tenant_id_here
REACT_APP_REDIRECT_URI=http://localhost:8000

# Backend Azure AD Configuration (Optional - for enhanced validation)
AZURE_CLIENT_SECRET=your_azure_client_secret_here

# Anthropic API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: LangChain Tracing (for debugging)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_PROJECT=excom-ai-chat

# JIRA Configuration (Optional - for MCP tools)
JIRA_SERVER=https://your-company.atlassian.net
JIRA_EMAIL=your-email@company.com
JIRA_API_TOKEN=your-jira-api-token-here
JIRA_JQL=project = MYPROJECT AND issuetype in (Story, Task, Bug)

# Freshservice Configuration (Optional - for MCP tools)
FRESHSERVICE_DOMAIN=your-freshservice-subdomain
FRESHSERVICE_API_KEY=your-freshservice-api-key-here